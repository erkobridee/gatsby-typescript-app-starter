{
	"name": "gatsby-typescript-app-starter",
	"private": true,
	"description": "A simple typescript application starter to get up and developing quickly with Gatsby",
	"version": "0.4.0",
	"author": "Erko Bridee <erko.bridee@gmail.com>",
	"dependencies": {
		"abortcontroller-polyfill": "^1.3.0",
		"classnames": "^2.2.6",
		"css-vars-ponyfill": "^2.0.2",
		"qs": "^6.7.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-helmet": "^5.2.1",
		"react-intl": "^4.1.1",
		"react-redux": "^7.1.0",
		"redux": "^4.0.1",
		"redux-persist": "^6.0.0",
		"redux-thunk": "^2.3.0",
		"whatwg-fetch": "^3.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@formatjs/intl-relativetimeformat": "^4.5.1",
		"@testing-library/jest-dom": "^5.1.1",
		"@testing-library/react": "^10.0.1",
		"@types/classnames": "^2.2.8",
		"@types/jest": "^25.1.4",
		"@types/node": "^13.9.2",
		"@types/qs": "^6.5.3",
		"@types/reach__router": "^1.2.4",
		"@types/react": "^16.8.21",
		"@types/react-dom": "^16.8.4",
		"@types/react-helmet": "^5.0.8",
		"@types/react-redux": "^7.1.0",
		"@typescript-eslint/eslint-plugin": "^2.24.0",
		"@typescript-eslint/parser": "^2.24.0",
		"babel-jest": "^25.1.0",
		"babel-plugin-react-intl": "^6.2.0",
		"babel-preset-gatsby": "^0.3.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-prettier": "^3.1.0",
		"express": "^4.17.1",
		"fs-extra": "^9.0.0",
		"gatsby": "^2.13.41",
		"gatsby-image": "^2.2.7",
		"gatsby-plugin-create-client-paths": "^2.1.5",
		"gatsby-plugin-manifest": "^2.2.4",
		"gatsby-plugin-offline": "^3.1.0",
		"gatsby-plugin-react-helmet": "^3.1.2",
		"gatsby-plugin-sass": "^2.1.3",
		"gatsby-plugin-sharp": "^2.2.9",
		"gatsby-plugin-typescript": "^2.1.2",
		"gatsby-source-filesystem": "^2.1.6",
		"gatsby-transformer-sharp": "^2.2.4",
		"gh-pages": "^2.0.1",
		"identity-obj-proxy": "^3.0.0",
		"intl-pluralrules": "^1.1.1",
		"jest": "^25.1.0",
		"jest-junit": "^10.0.0",
		"node-sass": "^4.12.0",
		"npm-check": "^5.9.2",
		"npm-run-all": "^4.1.5",
		"postcss-preset-env": "^6.6.0",
		"prettier": "^1.18.2",
		"react-intl-translations-manager": "^5.0.3",
		"request": "^2.88.0",
		"svgo": "^1.3.2",
		"ts-jest": "^25.2.1",
		"typescript": "^3.5.2",
		"webfonts-generator": "^0.4.0",
		"yargs": "^15.3.1"
	},
	"resolutions": {
		"browserslist": "4.6.2",
		"caniuse-lite": "1.0.30000974"
	},
	"keywords": [
		"gatsby",
		"starter",
		"sass",
		"typescript",
		"dotenv",
		"proxy support",
		"jest",
		"unit tests",
		"unit tests coverage"
	],
	"license": "MIT",
	"scripts": {
		"fonticons:clean-scss": "node scripts/fs/rm src/assets/styles/generated/",
		"fonticons:clean-generated": "node scripts/fs/rm .build/icons/",
		"fonticons:clean-static": "node scripts/fs/rm static/generated/",
		"fonticons:clean": "npm-run-all -p fonticons:clean-generated fonticons:clean-static fonticons:clean-scss",
		"fonticons:compress-mkdir": "node scripts/fs/mkdir .build/icons/compressed",
		"fonticons:compress-run": "svgo --config=svgo-config.json --enable=removeStyleElement --enable=removeScriptElement --enable=sortAttrs src/assets/icons/*.svg -o .build/icons/compressed > /dev/null",
		"fonticons:compress": "npm-run-all -s fonticons:compress-mkdir fonticons:compress-run",
		"fonticons:generate": "node scripts/font-generator static/generated/fonticons/ -f fonticons",
		"fonticons:move-scss": "node scripts/fs/mv static/generated/fonticons/fonticons.scss src/assets/styles/generated/fonticons.scss",
		"fonticons": "npm-run-all fonticons:clean fonticons:compress fonticons:generate fonticons:move-scss",
		"translations:extract": "TRANSLATIONS=true babel src --extensions '.ts,.tsx' --out-file .build/i18nExtractedMessages/compiled.js",
		"translations:runner": "node scripts/translation-runner.js",
		"translations": "npm-run-all -s translations:extract translations:runner",
		"generate": "npm-run-all -p fonticons translations",
		"format": "prettier --write 'src/**/*.{ts,tsx}'",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"type-check": "tsc",
		"gatsby-clean": "gatsby clean",
		"gatsby-develop": "gatsby develop -H 0.0.0.0",
		"gatsby-build": "gatsby build --prefix-paths",
		"gatsby-serve": "node scripts/proxied-http-server.js",
		"clean:build": "node scripts/fs/rm .build/",
		"clean:coverage": "node scripts/fs/rm coverage/",
		"clean": "npm-run-all -p clean:coverage clean:build fonticons:clean gatsby-clean",
		"unit-tests": "jest --passWithNoTests",
		"check": "npm-run-all -s type-check lint",
		"base": "npm-run-all -s generate check",
		"develop": "npm-run-all -s base gatsby-develop",
		"start": "npm run develop",
		"build": "npm-run-all -s clean base unit-tests gatsby-build",
		"serve": "ACTIVE_ENV=development npm-run-all -s build gatsby-serve",
		"gh-deploy": "gh-pages -d public",
		"deploy": "PREFIX_PATH=true npm-run-all -s build gh-deploy",
		"upgrade-interactive": "npm-check --update"
	},
	"browserslist": [
		">1%",
		"ie>=11",
		"iOS >= 9",
		"Android >= 4.4",
		"not op_mini all"
	],
	"gatsby": {
		"pathPrefix": "gatsby-typescript-app-starter"
	},
	"jest-junit": {
		"suiteName": "jest tests",
		"outputDirectory": "coverage",
		"outputName": "junit.xml",
		"uniqueOutputName": "false",
		"classNameTemplate": "{classname}",
		"titleTemplate": "{title}",
		"ancestorSeparator": " â€º ",
		"usePathForSuiteName": "true"
	}
}
